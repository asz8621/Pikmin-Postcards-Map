    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "sass-embedded": "^1.87.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.2.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^3.2.4"
  }
